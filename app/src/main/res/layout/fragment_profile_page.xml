<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ui.profile.ProfilePage"
    android:layout_marginTop="45dp">

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="44dp"
        android:layout_height="45dp"
        android:layout_marginStart="17dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="9dp"
        android:clickable="true"
        app:layout_constraintBottom_toTopOf="@+id/editTextText2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.935"
        app:layout_constraintStart_toEndOf="@+id/username"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/settingsbuttonimg"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="88dp"
        android:layout_height="89dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="7dp"
        android:layout_marginBottom="18dp"
        app:layout_constraintBottom_toTopOf="@+id/editTextText4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/profilepic"
        tools:ignore="MissingConstraints"
        tools:srcCompat="@drawable/profilepic" />

    <TextView
        android:id="@+id/username"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="45dp"
        android:layout_marginTop="28dp"
        android:layout_marginBottom="3dp"

        android:ems="10"
        android:inputType="text"
        android:minHeight="48dp"
        android:text="Username: USER1"
        android:textSize="18dp"
        app:layout_constraintBottom_toTopOf="@+id/editTextText2"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.41000003"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/editTextText2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="45dp"
        android:layout_marginTop="2dp"
        android:layout_marginBottom="2dp"
        android:ems="10"
        android:inputType="text"
        android:minHeight="48dp"
        android:text="Listings: 0"
        android:textSize="18dp"
        app:layout_constraintBottom_toTopOf="@+id/editTextText3"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/username"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/editTextText3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="45dp"
        android:layout_marginTop="2dp"
        android:layout_marginBottom="2dp"
        android:ems="10"
        android:inputType="text"
        android:minHeight="48dp"
        android:text="Rating: (No Ratings Yet)"
        android:textSize="18dp"
        app:layout_constraintBottom_toTopOf="@+id/editTextText4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/editTextText2"
        app:layout_constraintVertical_bias="0.454"
        tools:ignore="MissingConstraints" />


    <TextView
        android:id="@+id/editTextText4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="125dp"
        android:layout_marginBottom="20dp"
        android:ems="10"
        android:inputType="text"
        android:minHeight="48dp"
        android:text="My Inventory: "
        android:textSize="22dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        tools:ignore="MissingConstraints" />

    <androidx.cardview.widget.CardView
        android:id="@+id/newPost"
        android:layout_width="match_parent"
        android:layout_height="250dp"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@id/editTextText4"
        app:cardBackgroundColor="#EFED7C">


        <ImageView
            android:id="@+id/imageView"
            android:layout_width="115dp"
            android:layout_height="wrap_content"
            android:layout_margin="20dp"
            app:srcCompat="@drawable/milkyellowbg" />

        <TextView
            android:id="@+id/ItemName"
            android:layout_width="250dp"
            android:layout_height="48dp"
            android:layout_gravity="right|top"
            android:layout_marginHorizontal="10dp"
            android:layout_marginTop="20dp"
            android:layout_marginBottom="20dp"
            android:ems="10"
            android:inputType="text"
            android:text="Item name here"
            android:textAppearance="@style/TextAppearance.AppCompat.Large" />

        <CheckBox
            android:id="@+id/checkBox"
            android:layout_gravity="right|top"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            />

        <TextView
            android:id="@+id/ItemQuant"
            android:layout_width="250dp"
            android:layout_height="48dp"
            android:layout_gravity="top|right"
            android:layout_marginHorizontal="10dp"
            android:layout_marginTop="55dp"
            android:ems="10"
            android:inputType="text"
            android:text="X lbs." />

        <TextView
            android:id="@+id/ItemExpiry"
            android:layout_width="250dp"
            android:layout_height="50dp"
            android:layout_gravity="top|right"
            android:layout_marginHorizontal="10dp"
            android:layout_marginTop="95dp"
            android:ems="10"
            android:inputType="text"
            android:minHeight="48dp"
            android:text="Expires: XX/XX/XXXX" />

        <TextView
            android:id="@+id/TagsText"
            android:layout_width="250dp"
            android:layout_height="48dp"
            android:layout_gravity="top|right"
            android:layout_marginHorizontal="10dp"
            android:layout_marginTop="155dp"
            android:ems="10"
            android:inputType="text"
            android:text="tag_1, tag_2, tag_3"
            tools:ignore="DuplicateClickableBoundsCheck" />

        <TextView
            android:id="@+id/DescText"
            android:layout_width="250dp"
            android:layout_height="50dp"
            android:layout_gravity="top|right"
            android:layout_marginTop="200dp"
            android:layout_marginHorizontal="10dp"
            android:layout_marginBottom="20dp"
            android:ems="10"
            android:inputType="text"
            android:text="Description here"
            tools:ignore="DuplicateClickableBoundsCheck" />
    </androidx.cardview.widget.CardView>

    <TextView
        android:id="@+id/editTextText5"
        android:layout_width="377dp"
        android:layout_height="56dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="68dp"
        android:layout_marginEnd="7dp"
        android:layout_marginBottom="294dp"
        android:inputType="text"
        android:text="Your Inventory is empty. Hit the + to add an item!"
        android:textSize="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <ListView
        android:id="@+id/listView"
        android:layout_width="420dp"
        android:layout_height="300dp"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="2dp"
        android:background="#EFED7C"
        android:inputType="text"
        android:scrollbars="vertical"
        android:textSize="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/newPost"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints,TouchTargetSizeCheck" />


</androidx.constraintlayout.widget.ConstraintLayout>